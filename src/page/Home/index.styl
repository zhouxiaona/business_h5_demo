#Home{
    width 100vw;
    height: 100vh;
    overflow: hidden
    img{
        display: block
    }
    .signPre{
        width: 100vw
        min-height: 100vh
        background: #5E189D
        overflow: scroll
        position: relative
        .bg{
            width: 100%
        }
        .bgCont{
            width: 100%
            height: auto
            box-sizing: border-box
            padding-bottom: 24.5vw
            position: absolute
            top: 0
            left: 0
            padding-top: 2.9vw
            .titleBg{
                width: 71.7vw
                margin: 0 auto
            }
            .formList{
                width: 100%
                box-sizing: border-box
                padding: 0 6.4vw
                .inpWrap{
                    display: flex
                    justify-content: space-between
                    align-items: center
                    margin-bottom: 4vw
                    .wrapL{
                        // height: 11vw
                        display: flex
                        align-items: center
                        font-size:4.0vw;
                        font-family:PingFang SC;
                        color:rgba(255,255,255,1);
                        &::before{
                            display: block
                            content:''
                            width:1.1vw;
                            height:1.1vw;
                            background:rgba(255,255,255,1);
                            border-radius:50%;
                            margin-right: 2.1vw
                        }
                    }
                    .wrapR{
                        width:63.7vw;
                        height:11.0vw;
                        box-sizing: border-box
                        padding-left: 5.1vw
                        background:rgba(255,255,255,1);
                        border-radius:1.5vw;
                        display:flex
                        align-items: center
                        justify-content: space-between
                        input{
                            border:0
                            width:50vw;
                            height:9.0vw;
                            font-size:4.0vw;
                            font-family:PingFang SC;
                            font-weight:bold;
                            display:flex
                            align-items: center
                            color:rgba(73,73,73,1);
                            box-sizing: border-box
                        }
                        ::-webkit-input-placeholder { /* Chrome/Opera/Safari */ 
                            font-size:4.0vw;
                            font-family:PingFang SC;
                            font-weight:bold;
                            color:rgba(73,73,73,1);
                        }
                        ::-moz-placeholder { /* Firefox 19+ */  
                            font-size:4.0vw;
                            font-family:PingFang SC;
                            font-weight:bold;
                            color:rgba(73,73,73,1);
                        }
                        :-ms-input-placeholder { /* IE 10+ */ 
                            font-size:4.0vw;
                            font-family:PingFang SC;
                            font-weight:bold;
                            color:rgba(73,73,73,1);
                        }
                        :-moz-placeholder { /* Firefox 18- */ 
                            font-size:4.0vw;
                            font-family:PingFang SC;
                            font-weight:bold;
                            color:rgba(73,73,73,1);
                        }
                        .phoneInp{
                            width: 30vw
                        }
                        .codeTime{
                            width: 25vw
                            height:9.0vw;
                            display:flex
                            align-items: center
                            justify-content: center
                            position: relative
                            font-size:4.0vw;
                            font-family:PingFang SC;
                            font-weight:bold;
                            color:rgba(94,25,157,1);
                            &::before{
                                display: block
                                content: ''
                                width: 1px
                                height: 6.6vw
                                background: rgba(73,73,73,1)
                                position: absolute
                                left: 0
                                top: 0
                                bottom: 0
                                margin: auto 0
                            }
                        }
                        .pickerWrap{
                            width:50vw;
                            height:11.0vw;
                            font-size:4.0vw;
                            font-family:PingFang SC;
                            font-weight:bold;
                            display:flex
                            align-items: center
                            color:rgba(73,73,73,1);
                            box-sizing: border-box
                        }
                        .narrow{
                            width: 3.2vw
                            margin-right: 4vw
                        }
                    }
                    .selectR{
                        width:25.9vw;
                        height:6.7vw;
                        border:0.3vw solid rgba(255,255,255,1);
                        border-radius:0.9vw;
                        position: relative
                        select{
                            width:100%;
                            height:100%;
                            box-sizing: border-box
                            padding-left: 2.7vw
                            border: none 
                            outline: none
                            background: transparent
                            font-size:4.0vw;
                            font-family:PingFang SC;
                            color:rgba(255,255,255,1);
                            appearance: none;
                            -webkit-appearance: none;
                            -moz-appearance: none;
                            option{
                                font-size:4.0vw;
                                font-family:PingFang SC;
                                color:rgba(0,0,0,1);
                            }
                        }
                        .narrowW{
                            width: 2.8vw
                            position: absolute
                            right: 2vw
                            top: 40%
                        }

                    }
                }
                .inpWrapSlect{
                    margin-top: 6vw
                }
                .lineWrap{
                    padding: 0 4vw
                    .line{
                        width: 100%
                        height: 10.4vw
                        display: flex
                        justify-content: space-between
                        align-items: center
                        border-bottom: 0.3vw solid rgba(128,98,197,1);
                        .lineL{
                            font-size:3.5vw;
                            font-family:PingFang SC;
                            font-weight:400;
                            color:rgba(255,255,255,1);
                            white-space: nowrap
                        }
                        .gou,.circle{
                            width: 3vw
                            height: 3vw
                        }
                        .circle{
                            box-sizing: border-box
                            border: 0.2vw solid #ffffff
                            border-radius: 50%
                        }
                    }
                }
            }
        }
        .btnWrap{
            width: 100%
            height: 24.5vw
            display: flex
            justify-content: center
            align-items: center
            position: fixed
            bottom: 0
            left: 0
            background: #5E189D
            .btn{
                width: 41.8vw
            }

        }
    }
    .signEd{
        width: 100vw
        height: 100vh
        overflow: hidden
        .successWrap{
            width: 100vw
            padding-top: 18.4vw
            display: flex
            flex-direction: column
            align-items: center
            .success{
                width: 22.8vw
            }
            .tip1{
                font-size:7.5vw;
                font-family:PingFang SC;
                font-weight:bold;
                color:rgba(0,0,0,1);  
                margin-bottom: 2.1vw  
            }
            .tip2{
                font-size:4.0vw;
                font-family:PingFang SC;
                color:rgba(0,0,0,1);
            }
        }
        .banner{
            width: 94.5vw
            margin: 54.7vw auto 0
        }
    }
}